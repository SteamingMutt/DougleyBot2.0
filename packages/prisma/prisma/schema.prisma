// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Tag {
  id       Int    @id @default(sequence())
  name     String @unique
  content  String
  authorId BigInt
}

model Guild {
  id BigInt @id
}
